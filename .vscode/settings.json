{
    "editor.fontSize": 15,
    "window.zoomLevel": 1.25,
    "files.exclude": {
        "_build": true
    },
    "editor.fontFamily": "Monaspace Neon Var, Fira Code, Monaco, monospace",
    "editor.fontVariations": "'wght' 350",
    "editor.fontLigatures": "'calt', 'liga', 'ss03', 'ss04', 'ss06'",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "storage,entity,variable",
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "terminal.integrated.fontFamily": "Monaspace Neon Var, Fira Code, Monaco, monospace",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": 500,
    "terminal.integrated.fontWeightBold": 800
}